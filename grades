#include <stdio.h>
int main(){
    int number;
    printf("enetr a two-digit number:" );
    scanf("%d",&number);
    printf("letter grade:");
    if(number<=100&&number>=90)printf("A");
    else if(number<=89&&number>=80)printf("B");
    else if(number<=79&&number>=70)printf("C");
    else if(number<=69&&number>=60)printf("D");
    else if(number<=59&&number>=0)printf("E");
    else printf("error");
    return 0;
}
