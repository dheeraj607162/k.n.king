import java.util.Scanner;
class welcome{
    public static void main(String[] aurgs){
          int a;
          boolean leap=false;
          Scanner sc=new Scanner(System.in);
          System.out.print("enter year");
          a=sc.nextInt();
          if(a%4==0){
              if(a%100==0){
                  if(a%400==0){
                      leap=true;
                  }
              }
              else{
                  leap=true;
              }
          }
          else{
              leap=false;
          }
          if(leap==true){
              System.out.print("it is leap year:"+a);
          }
          else{
              System.out.print("not a leap year:"+a);
    }
    }
}
