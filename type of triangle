#include<stdio.h>
int main(){
    int a,b,c;
    printf("enter a value");
    scanf("%d",&a);
    printf("enter b value");
    scanf("%d",&b);
    printf("enter c value");
    scanf("%d",&c);
    if(a==b&&a==c&&b==c)printf("equilateral triangle");
    else if(a!=b&&a!=c&&b!=c)printf("scalen triangle");
    else printf("isoseles triangle");
    return 0;
}
