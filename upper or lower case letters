#include<stdio.h>
int main(){
    int character;
    printf("enter any character:");
    scanf("%c",&character);
    if(character>=66&&character<=90)printf("you entered character is uppercase");
    else if(character>=97&&character<=122)printf("you entered char is lowercase");
    else if(character>=48&&character<=57)printf("you enterd a number",character);
    else printf("you entered spl character");
    return 0;
}
